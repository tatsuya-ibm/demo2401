            <xsd:extension base="com.ibm.ws.app.manager.webappcfg">
                <xsd:attribute name="id" type="factoryIdType" use="optional">
                    <xsd:annotation>
                        <xsd:documentation>固有の構成 ID。</xsd:documentation>
